<isdecorate template="common/layout/page">
    <p>Hello <isprint value="${pdict.welcomeMsg}" encoding="on" /></p>

    <isscript>
        var items = ["one", "two", "three"];
    </isscript>

    <isloop items="${items}" var="item" status="index">
        <isif condition="${index.first || index.last}">
            <p><strong>${item}</strong></p>
        <iselse/>
            <p>${item}</p>
        </isif>
    </isloop>

    <isset name="changeScope" value="1" scope="pdict" />
    <isprint value="${new Date()}" encoding="on" style="DATE_LONG"/>
    <isinclude url="${URLUtils.url('TestController-Include')}" />
</isdecorate>
